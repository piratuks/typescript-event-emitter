{
  "name": "typescrpt-event-emitter",
  "version": "1.0.0",
  "description": "Versatile and feature-rich TypeScript library for event management, providing a solid foundation for building event-driven applications in TypeScript.",
  "author": "Evaldas Laureckas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "pack": "npm pack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "EventEmitter",
    "Events",
    "addEventListener",
    "addListener",
    "emit",
    "emits",
    "emitter",
    "event",
    "once",
    "subscribe",
    "Typescrpt EventEmitter",
    "Typescrpt Events Management"
  ],
  "bugs": {
    "url": "https://github.com/piratuks/typescript-event-emitter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/piratuks/typescript-event-emitter.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3"
  }
}
